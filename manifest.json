{
  "manifest_version": 3,
  "name": "Keynest",
  "version": "1.0",
  "description": "Генератор запоминающихся паролей.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://keynest-was2.onrender.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}